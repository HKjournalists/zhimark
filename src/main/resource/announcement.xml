<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.mvc.repository.AnnouncementRepository">
    <select id="getAllAnnouncements" resultType="com.spring.mvc.model.AnnouncementModel">
        select * from ims_eso_sale_announcement order by createdDate desc limit 0,5;
    </select>
</mapper>